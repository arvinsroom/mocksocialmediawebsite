  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack serve --mode development",
    "clean": "rm -rf ./migrate && mkdir -p ./migrate",
    "babel-migrate-transpile": "babel ./database --out-dir migrate --plugins=@babel/plugin-syntax-dynamic-import,@babel/plugin-proposal-object-rest-spread",
    "run-migrations": "node ./migrate/run-migrations.js",
    "migrate": "npm run clean && npm run babel-migrate-transpile && npm run run-migrations"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
